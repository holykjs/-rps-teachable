# Production Environment Configuration
# Copy this to .env.production.local and update with your production values

# AI Model Configuration
VITE_MODEL_URL=https://teachablemachine.withgoogle.com/models/GuM5MHK94/
VITE_CONFIDENCE_THRESHOLD=0.6
VITE_WIN_SCORE=5

# Multiplayer Server Configuration (Update with your Railway deployment URL)
# After deploying to Railway, replace with your actual URL:
# VITE_SERVER_URL=https://your-rps-server.railway.app
VITE_SERVER_URL=https://your-production-server.com
VITE_RECONNECT_TIMEOUT=5000

# Performance Settings (Optimized for production)
VITE_GESTURE_HISTORY_SIZE=6
VITE_STABILITY_FRAMES=3
VITE_DETECTION_INTERVAL=150

# UI Configuration
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SOUND=true
VITE_THEME=dark

# Security (for production deployment)
VITE_ENABLE_DEBUG=false
VITE_LOG_LEVEL=error
